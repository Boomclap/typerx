    "client:lint": "ng lint --fix",
    "client:test": "ng test",
    "debug": "nodemon --inspect .",
    "build": "npm run clean && tsc --p src/server/tsconfig.json && npm run swagger",
    "e2e": "ng e2e",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "ng": "ng",
    "server:lint": "tslint  --project ./tsconfig.json './src/server/**/*.ts' -c tslint.server.json --fix",
    "server:test": "jasmine-ts ./src/server/**/*.spec.ts",
    "server:watch": "nodemon",
    "server:build": "npm run clean && tsc -p tsconfig.server.json && npm run swagger",
    "start": "concurrently --kill-others \"npm run server:watch\" \"ng serve --proxy-config proxy.conf.json\"",
    "swagger": "swaggerGen -c ./swagger.config.json",
    "prettify": "pretty-quick"
